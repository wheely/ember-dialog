{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "source": {
    "include": [
      "app",
      "addon"
    ],
    "includePattern": ".+\\.js(doc)?$",
    "exclude": [
      "app/styles",
      "app/templates"
    ],
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "theme": "cosmo",
    "systemName": "ember-dialog",
    "sort": "version, since, longname"
  },
  "opts": {
    "encoding": "utf8",
    "template": "node_modules/ink-docstrap/template",
    "destination": "docs",
    "recurse": true,
    "access": "all",
    "readme": "README.md"
  }
}
